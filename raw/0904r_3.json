    },
    {
      "value": "",
      "offset": 862
    },
    {
      "value": "",
      "offset": 863
    },
    {
      "value": "　初めての行為を終えた僕たち。",
      "offset": 864
    },
    {
      "value": "　何となくそのまま離れがたくて、こうして一緒にいる。",
      "offset": 895
    },
    {
      "value": "「……なんかいいですね、こういうのって」",
      "offset": 948
    },
    {
      "value": "「ええ……心がぽかぽかしてきます」",
      "offset": 989
    },
    {
      "value": "「わたしも……」",
      "offset": 1024
    },
    {
      "value": "　語り合うべきことは、いっぱいあるけれど。",
      "offset": 1041
    },
    {
      "value": "　でも、今はそういうものはすべて忘れて、ただ見つめ合っていたかった。",
      "offset": 1084
    },
    {
      "value": "「こんな日が、これからも続くといいな……」",
      "offset": 1153
    },
    {
      "value": "「大丈夫ですよ、きっと。僕と凛さんならね」",
      "offset": 1196
    },
    {
      "value": "「……はいっ」",
      "offset": 1239
    },
    {
      "value": "　穏やかな気分。",
      "offset": 1254
    },
    {
      "value": "　本当に、このまま続いて欲しいと願う。",
      "offset": 1271
    },
    {
      "value": "「でも、わたし……えっちしちゃったんですね……なんだかビックリです」",
      "offset": 1310
    },
    {
      "value": "「ぼ、僕も……凛さんと今日いきなりこうなるなんて思ってなかったから……」",
      "offset": 1379
    },
    {
      "value": "「え？　ってことは、いつかはこうなるって？」",
      "offset": 1452
    },
    {
      "value": "「…………凛さんのいじわる」",
      "offset": 1497
    },
    {
      "value": "「ふふっ、いいんですよ、わたしだって似たようなものですし」",
      "offset": 1526
    },
    {
      "value": "　そう言って微笑む凛さん。今更だけど、やっぱり可愛い。",
      "offset": 1585
    },
    {
      "value": "「お互いはじめてだったのに、いっぱいしちゃいましたね」",
      "offset": 1640
    },
    {
      "value": "「あっ……」",
      "offset": 1695
    },
    {
      "value": "「えっちな凛さんが可愛かったから、僕もつい調子に乗っちゃって」",
      "offset": 1708
    },
    {
      "value": "「……あ、あんまり思い出させないで下さい。恥ずかしいですから」",
      "offset": 1771
    },
    {
      "value": "「でも、またしましょうね。凛さんが感じてるとこ、また見せて欲しいから」",
      "offset": 1834
    },
    {
      "value": "「も、もう、静真さんったらっ」",
      "offset": 1905
    },
    {
      "value": "「……ダメですか？」",
      "offset": 1936
    },
    {
      "value": "「だ、ダメな訳ないじゃないですか……わ、わたしもまた、静真さんにして欲しいし……」",
      "offset": 1957
    },
    {
      "value": "「って、何言わせるんですかぁ、もうっ！！」",
      "offset": 2040
    },
    {
      "value": "「あははははっ」",
      "offset": 2083
    },
    {
      "value": "　こうして、静かに夜は更けていく。",
      "offset": 2100
    },
    {
      "value": "　朝までもうそんなに時間はないけれど、でも……",
      "offset": 2135
    },
    {
      "value": "　せめて、夜が明けるまではこうして２人で過ごそうと思った。",
      "offset": 2182
    }
  ]
}